	keywords = {Fundamental optical physics},
	pages = {531--534},
	file = {Full Text PDF:/home/davidruffner/.mozilla/firefox/avqs29n9.default/zotero/storage/UVCQISZP/Chen et al. - 2011 - Optical pulling force.pdf:application/pdf}
},

@article{soong_theoretical_2010,
