@article{costa_de_beauregard_mechanical_1976,
	title = {Mechanical interpretation of shifts in total reflection of spinning particles},
	volume = {36},
	issn = {0369-3554},
	url = {http://www.springerlink.com/content/334q9938323w4033/abstract/},
	doi = {10.1007/BF02725231},
	abstract = {A simple derivation of the shifts occurring in total reflection is presented by means of the relativistic dynamics of spinning extended structures or spinning particles. This analysis is consonant with remarks due to von Laue and to works by various authors on the dynamics of spinning particles. In the case of total reflection at a plane interface, one speaks of the longitudinal, or Goos-Hänchen's, and of the transverse, or Imbert's, shifts. These are in a one-to-one association with the observations of the so-called tangential and sagittal focal lines of geometrical optics. When the quanta associated with the waves have nonzero spin, these focal lines possess a fine structure displaying a set of orthogonal-polarization eigenstates. This has been experimentally demonstrated in the total reflection of light beams. Here we extend our previous theory (given for the case of light) by merely interpreting the formulae recently derived by Fradkin and Kashuba for the total reflection of a Dirac electron.},
	number = {2},
	urldate = {2012-09-07},
	journal = {Il Nuovo Cimento B (1971-1996)},
	author = {Costa de Beauregard, O.},
	year = {1976},
	keywords = {Physics and Astronomy},
	pages = {119--130},
	file = {SpringerLink Full Text PDF:/home/davidruffner/.mozilla/firefox/avqs29n9.default/zotero/storage/G4GRIKXK/Costa de Beauregard - 1976 - Mechanical interpretation of shifts in total refle.pdf:application/pdf;SpringerLink Snapshot:/home/davidruffner/.mozilla/firefox/avqs29n9.default/zotero/storage/96P4RB6Q/abstract.html:text/html}
},

@article{rohrbach_optical_2001,
